<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo">
  <StackLayout>
    <Label text="ListenerId" class="message" textWrap="true"/>
    <TextView text="{{ listenerId }}" />

    <Label text="txPower" class="message" textWrap="true"/>
    <TextView text="{{ txPower }}" />

    <Label text="ServerUrl" class="message" textWrap="true"/>
    <TextView text="{{ serverUrl }}" />

    <Button text="Submit" tap="{{onSubmit}}"/>
    
  </StackLayout>
</Page>